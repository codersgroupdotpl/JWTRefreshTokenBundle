{
  "name" : "codersgroupdotpl/jwt-refresh-token-bundle",
  "description" : "Implements a refresh token system over Json Web Tokens in Symfony",
  "type" : "symfony-bundle",
  "authors" : [{
    "name" : "Marcos GÃ³mez Vilches",
    "email" : "marcos@gesdinet.com"
  }],
  "keywords" : [
    "jwt refresh token bundle symfony json web"
  ],
  "license" : [
    "MIT"
  ],
  "require" : {
    "php": ">=8.0",
    "doctrine/persistence": "^1.3.3|^2.0|^3.0",
    "lexik/jwt-authentication-bundle": "^2.0|^3.0",
    "symfony/config": "^6.2",
    "symfony/console": "^6.2",
    "symfony/dependency-injection": "^6.2",
    "symfony/deprecation-contracts": "^2.1|^3.0",
    "symfony/event-dispatcher": "^6.2",
    "symfony/http-foundation": "^6.2",
    "symfony/http-kernel": "^4.4|^5.4|^6.0",
    "symfony/polyfill-php80": "^1.15",
    "symfony/property-access": "^6.2",
    "symfony/security-bundle": "^6.2",
    "symfony/security-core": "^6.2",
    "symfony/security-http": "^6.2"
  },
  "require-dev": {
    "doctrine/cache": "^1.11|^2.0",
    "doctrine/mongodb-odm": "^2.2",
    "doctrine/orm": "^2.7",
    "matthiasnoback/symfony-config-test": "^4.2",
    "matthiasnoback/symfony-dependency-injection-test": "^4.2",
    "phpunit/phpunit": "^9.5",
    "symfony/cache": "^6.2",
    "symfony/security-guard": "^4.4|^5.4"
  },
  "conflict": {
    "doctrine/mongodb-odm": "<2.2",
    "doctrine/orm": "<2.7"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    },
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "Gesdinet\\JWTRefreshTokenBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Gesdinet\\JWTRefreshTokenBundle\\Tests\\": "Tests/"
    }
  },
  "extra" : {
    "branch-alias" : {
      "dev-master" : "1.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
